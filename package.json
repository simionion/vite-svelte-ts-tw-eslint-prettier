{
  "name": "fss",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "npm run lint && vite build",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{css,html,js,ts,svelte}'",
    "lint": "eslint --fix src --ext .js,.ts,.svelte",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --fix --cache --color",
    "*.{css,html,js,ts,svelte}": "prettier --write"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^1.4.1",
    "@sveltejs/vite-plugin-svelte": "^1.4.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@vitest/coverage-c8": "^0.25.4",
    "@vitest/ui": "^0.23.4",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-better-mutation": "^1.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "ramda": "^0.28.0",
    "svelte": "^3.53.1",
    "svelte-gestures": "^1.4.1",
    "svelte-preprocess": "^4.10.7",
    "terser": "^5.16.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.5",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  }
}
